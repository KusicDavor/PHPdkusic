{{ form_start(form) }}
{% if not registrationForm.vars.value %}
	{# Show password field only on the create page #}
	{{ form_row(registrationForm.plainPassword, { 'label': 'Password' }) }}
{% endif %}
<div style="margin-left: 10px">
	{{ form_widget(form) }}
</div>
<button style="margin-left: 10px; font-size: 14px" class="btn">{{ button_label|default('Save') }}</button>
<a style="margin-top: 10px; margin-left: 10px; font-size: 13px" class="btn btn-primary" href=" {{ path('app_user_index') }} ">Back to list</a>
{{ form_end(form) }}
