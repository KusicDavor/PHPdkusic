{{ form_start(form) }}
<div style="margin-left: 10px">
	{{ form_widget(form) }}
</div>
<button style="margin-left: 10px; font-size: 14px" class="btn">{{ button_label|default('Save') }}</button>
<a style="font-size: 12px" class="btn btn-primary" href=" {{ path('app_team_index') }} ">Back to list</a>
{% if is_granted('ROLE_ADMIN') %}
	{% if app.request.attributes.get('_route')|split('_')|last in ['new', 'edit'] %}
		{{ include('team/_delete_form.html.twig') }}
	{% endif %}
{% endif %}
{{ form_end(form) }}
